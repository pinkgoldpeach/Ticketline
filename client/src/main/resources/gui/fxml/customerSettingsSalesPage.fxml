<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="customerSettingsPane" prefHeight="504.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.ticketline.client.gui.controller.CustomerSettingsSalesPageController">
    <children>
        <TableView fx:id="tableCustomerView" layoutX="10.0" layoutY="14.0" prefHeight="371.0" prefWidth="680.0" AnchorPane.bottomAnchor="119.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="14.0">
            <columns>
                <TableColumn fx:id="tableColumnFirstname" prefWidth="75.0" text="%customer.table.firstname" />
                <TableColumn fx:id="tableColumnLastname" prefWidth="75.0" text="%customer.table.lastname" />
                <TableColumn fx:id="tableColumnBirthdate" prefWidth="129.0" text="%customer.table.birthdate" />
                <TableColumn fx:id="tableColumnEmail" prefWidth="97.0" text="%customer.table.email" />
                <TableColumn fx:id="tableColumnPostalcode" prefWidth="50.0" text="%customer.table.postalcode" />
                <TableColumn fx:id="tableColumnCity" prefWidth="75.0" text="%customer.table.city" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button fx:id="btnEditCustomer" layoutX="164.0" layoutY="394.0" mnemonicParsing="false" onAction="#handleEditCustomer" prefHeight="27.0" prefWidth="118.0" text="\%generic.edit" AnchorPane.bottomAnchor="83.0" />
        <Button fx:id="btnNewCustomer" layoutX="425.0" layoutY="394.0" mnemonicParsing="false" onAction="#handleNewCustomer" prefHeight="27.0" prefWidth="70.0" text="\%generic.new" AnchorPane.bottomAnchor="83.0" />
        <Button fx:id="btnPrev" layoutX="289.0" layoutY="395.0" mnemonicParsing="false" onAction="#previousPage" text="&lt;" AnchorPane.bottomAnchor="84.0" />
        <Button fx:id="btnNext" layoutX="394.0" layoutY="395.0" mnemonicParsing="false" onAction="#nextPage" text="&gt;" AnchorPane.bottomAnchor="84.0" />
        <Label fx:id="pageText" layoutX="364.0" layoutY="398.0" text="100" AnchorPane.bottomAnchor="89.0" />
        <TextField fx:id="pageInput" layoutX="324.0" layoutY="393.0" prefHeight="27.0" prefWidth="35.0" AnchorPane.bottomAnchor="84.0" />
        <Button fx:id="buttonChooseCustomer" layoutX="14.0" layoutY="467.0" mnemonicParsing="false" onAction="#handleChooseCustomer" prefHeight="27.0" prefWidth="170.0" text="%generic.chooseCustomer" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="516.0" />
        <Button fx:id="buttonCancel" layoutX="603.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="27.0" prefWidth="90.0" text="%generic.cancel" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
    </children>
</AnchorPane>
