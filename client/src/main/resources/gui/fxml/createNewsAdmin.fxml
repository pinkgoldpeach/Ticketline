<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="newNewsPane" prefHeight="231.0" prefWidth="566.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.ticketline.client.gui.controller.NewNewsAdminController">
    <children>
        <Button fx:id="btnCancel" layoutX="455.0" layoutY="189.0" mnemonicParsing="false" onAction="#handleCancel" text="\%generic.cancel" />
        <Button fx:id="btnNewNews" layoutX="14.0" layoutY="189.0" mnemonicParsing="false" onAction="#handlePublishNewsMessage" text="\%news.publish" />
        <TextField fx:id="fieldNewsTitle" layoutX="14.0" layoutY="22.0" prefHeight="27.0" prefWidth="541.0" promptText="\%news.title" />
        <TextArea fx:id="areaNewsMessage" layoutX="14.0" layoutY="53.0" prefHeight="122.0" prefWidth="541.0" promptText="\%news.message" />
    </children>
</AnchorPane>
